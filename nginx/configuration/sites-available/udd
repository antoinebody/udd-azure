##
upstream jenkins {
    server 127.0.0.1:6001;
}
server {
    listen 80;
    server_name localhost;

    root /usr/share/nginx/welcome;



    index index.html index.htm;

    location /jenkins {
        proxy_pass http://jenkins;
        proxy_redirect     off;
        proxy_set_header   Host $host;
    }
}

